        </value>
    </customProperties>
    <decisions>
        <name>Is_this_survey_prefilled</name>
        <label>Is this survey prefilled?</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Survey_Response_Question_One</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No, it is not filled</defaultConnectorLabel>
        <rules>
            <name>Yes_it_is_already_completed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recordId.Survey_Completed__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Survey_Question_flow_for_Leads</targetReference>
            </connector>
            <label>Yes, it is already completed</label>
        </rules>
    </decisions>
    <dynamicChoiceSets>
        <name>Rating_Choice</name>
        <dataType>Picklist</dataType>
        <displayField xsi:nil="true"/>
        <object xsi:nil="true"/>
        <picklistField>Rate_us_out_of_5__c</picklistField>
        <picklistObject>Survey_Response__c</picklistObject>
    </dynamicChoiceSets>
    <environments>Default</environments>
    <formulas>
        <name>SurveyResponseName</name>
        <dataType>String</dataType>
        <expression>&apos;Survey Response for &apos; + {!recordId.Name}</expression>
    </formulas>
    <interviewLabel>Survey Questions Flow for Lead {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Use case 4- Screen flow - Survey Questions Flow for Lead</label>
