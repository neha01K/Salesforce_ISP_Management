        </inputAssignments>
        <inputAssignments>
            <field>How_can_we_reach_out__c</field>
            <value>
                <elementReference>Reach_out_Email.value</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Name</field>
            <value>
                <elementReference>SurveyResponseName</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Parent_Lead__c</field>
            <value>
                <elementReference>recordId.Id</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Rate_us_out_of_5__c</field>
            <value>
                <elementReference>rate_factor</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Total_Deal_Size__c</field>
            <value>
                <elementReference>Deal_Size</elementReference>
            </value>
        </inputAssignments>
        <object>Survey_Response__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
