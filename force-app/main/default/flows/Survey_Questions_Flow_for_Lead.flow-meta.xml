    <recordUpdates>
        <name>Update_Survey_Completed_to_True</name>
        <label>Update Survey Completed to True</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
